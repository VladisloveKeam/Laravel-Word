{
    "name": "maveius/word",
    "description": "An eloquent way of importing and exporting Word and RTF in Laravel 4 with the power of PHPWord",
    "license": "LGPL",
    "keywords": ["laravel", "phpword", "word", "rtf", "export", "import", "batch"],
    "authors": [
        {
            "name": "maveius.pl",
            "email": "maveilthain@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "phpoffice/phpword": "~1.8.0",
        "illuminate/cache": "~4.0|~5.0",
        "illuminate/config": "~4.0|~5.0",
        "illuminate/filesystem": "~4.0|~5.0",
        "illuminate/support": "~4.0|~5.0",
        "nesbot/carbon": "~1.0",
        "tijsverkoyen/css-to-inline-styles": "~1.5"
    },
    "require-dev": {
        "phpseclib/phpseclib": ">=0.3.7",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "~2.2.0"
    },
     "suggest": {
        "illuminate/http": "~4.0|~5.0",
        "illuminate/routing": "~4.0|~5.0",
        "illuminate/view": "~4.0|~5.0"
    },
    "autoload": {
        "classmap": [
            "src/maveius/Word",
            "tests/TestCase.php"
        ],
        "psr-0": {
            "maveius\\Word\\": "src/"
        }
    }
}
